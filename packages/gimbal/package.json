{
  "name": "@modus/gimbal",
  "version": "1.0.13",
  "description": "A CLI tool for monitoring web performance in modern web projects",
  "main": "lib/gimbal/src/modules.js",
  "bin": {
    "gimbal": "bin/index.js"
  },
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "scripts": {
    "build": "ttsc -p .",
    "build:check": "ttsc --noEmit -p .",
    "build:cleanup": "rimraf lib/gimbal-core",
    "build:watch": "ttsc --watch -p .",
    "link": "npm link",
    "lint": "eslint 'bin/**/*.js' 'src/**/*.ts'",
    "postbuild": "yarn run copy-static-assets && yarn run build:cleanup",
    "prepublish": "yarn build",
    "copy-static-assets": "copyfiles -u 1 src/**/*/*.{html,txt} lib/gimbal/src",
    "start": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "start:break": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "start:debug": "node --inspect -r ts-node/register -r tsconfig-paths/register src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModusCreateOrg/gimbal.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-performance",
    "ci",
    "performance",
    "webdevelopment",
    "javascript",
    "typescript",
    "lighthouse",
    "bundlesize",
    "audit",
    "react",
    "puppeteer"
  ],
  "contributors": [
    {
      "name": "Muhammad Ahsan Ayaz",
      "email": "ahsan.ayaz@moduscreate.com"
    },
    {
      "name": "Grgur de Grisogono",
      "email": "grgur@moduscreate.com"
    },
    {
      "name": "Matthew Kelch",
      "email": "matthew.kelch@moduscreate.com"
    },
    {
      "name": "Seth Lemmons",
      "email": "seth.lemmons@moduscreate.com"
    },
    {
      "name": "Mitchell Simoens",
      "email": "mitchell.simoens@moduscreate.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ModusCreateOrg/gimbal/issues"
  },
  "homepage": "https://github.com/ModusCreateOrg/gimbal#readme",
  "dependencies": {
    "@modus/gimbal-core": "1.0.13",
    "@octokit/rest": "16.27.3",
    "brotli-size": "0.1.0",
    "bytes": "3.1.0",
    "cli-table3": "0.5.1",
    "colors": "1.3.3",
    "commander": "2.20.0",
    "deepmerge": "3.2.0",
    "figlet": "1.2.3",
    "get-folder-size": "2.0.1",
    "get-port": "5.0.0",
    "globby": "9.2.0",
    "gzip-size": "5.1.1",
    "js-yaml": "3.13.1",
    "lighthouse": "5.1.0",
    "marked": "0.6.2",
    "minimatch": "3.0.4",
    "mkdirp": "0.5.1",
    "pidusage": "2.0.17",
    "puppeteer": "1.17.0",
    "read-pkg": "5.1.1",
    "serve-handler": "6.0.1",
    "strip-ansi": "5.2.0",
    "update-notifier": "3.0.0"
  },
  "devDependencies": {
    "@types/bytes": "3.0.0",
    "@types/copyfiles": "2.1.1",
    "@types/figlet": "1.2.0",
    "@types/get-folder-size": "2.0.0",
    "@types/globby": "9.1.0",
    "@types/js-yaml": "3.12.1",
    "@types/marked": "0.6.5",
    "@types/minimatch": "3.0.3",
    "@types/mkdirp": "0.5.2",
    "@types/pidusage": "2.0.1",
    "@types/puppeteer": "1.12.4",
    "@types/read-pkg": "4.0.0",
    "@types/update-notifier": "2.5.0",
    "copyfiles": "2.1.0"
  }
}
